{
  "manifest_version": 2,
  "name": "StartUp",
  "version": "0.1.1",

  "default_locale": "en",
  "description": "An extension designed to streamline your start up experience with Chrome!",
  "icons":{

  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "StartUp",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "storage"
  ],
  "short_name": "StartUp"
}
